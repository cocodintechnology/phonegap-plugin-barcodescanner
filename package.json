{
  "name": "@cocodintech/phonegap-plugin-barcodescanner",
  "version": "1.0.0",
  "description": "BarcodeScanner plugin is a fork of the original phonegap-plugin-barcodescanner, adapted for cordova-android 12.0.0. It allows scanning various barcode types using the device's camera and extracting encoded metadata for in-app processing.",
  "cordova": {
    "id": "phonegap-plugin-barcodescanner",
    "platforms": [
      "ios",
      "android",
      "windows",
      "browser"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cocodintechnology/phonegap-plugin-barcodescanner"
  },
  "keywords": [
    "ecosystem:cordova",
    "ecosystem:phonegap",
    "cordova-ios",
    "cordova-android",
    "cordova-windows",
    "cordova-browser",
    "cordova:plugin"
  ],
  "engines": {
    "cordova-android": ">=10.0.0 <=12.0.0"
  },
  "author": "Adobe PhoneGap Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cocodintechnology/phonegap-plugin-barcodescanner/issues"
  },
  "homepage": "https://github.com/cocodintechnology/phonegap-plugin-barcodescanner#readme",
  "scripts": {
    "test": "jasmine-node --color spec"
  },
  "devDependencies": {
    "jasmine-node": "1.14.5",
    "pluginpub": "^0.0.9"
  }
}
